    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.2.4",
    "@nestjs/serve-static": "^2.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/express-session": "^1.17.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.1",
    "express-session": "^1.17.3",
    "http-proxy-middleware": "^2.0.6",
